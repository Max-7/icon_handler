<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <LocalDebuggerCommand>C:\Windows\system32\regsvr32.exe</LocalDebuggerCommand>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerCommandArguments> /n /i:user /s $(TargetPath)</LocalDebuggerCommandArguments>
    <LocalDebuggerAttach>false</LocalDebuggerAttach>
    <LocalDebuggerDebuggerType>Auto</LocalDebuggerDebuggerType>
  </PropertyGroup>
</Project>